{
  "name": "unplugin-svelte-vapor",
  "description": "unplugin for svelte-vapor",
  "version": "0.0.4",
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/inclusion-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/inclusion-vapor.git",
    "directory": "packages/unplugin-svelte"
  },
  "keywords": [
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "transform"
  ],
  "homepage": "https://github.com/kazupon/inclusion-vapor#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "import": "./dist/rspack.js",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.js",
      "require": "./dist/webpack.cjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "import": "./dist/rollup.js",
      "require": "./dist/rollup.cjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "import": "./dist/esbuild.js",
      "require": "./dist/esbuild.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./*": "./*"
  },
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "build:fix": "esno scripts/postbuild.ts",
    "prepublishOnly": "npm run build",
    "start": "esno src/index.ts",
    "watch": "tsup --watch src"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@vue-vapor/shared": "latest",
    "debug": "^4.3.6",
    "svelte": "catalog:",
    "svelte-vapor-runtime": "workspace:*",
    "svelte-vapor-sfc-compiler": "workspace:*",
    "unplugin": "^1.5.1"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3",
    "@nuxt/schema": "^3",
    "@types/debug": "^4.1.12",
    "esbuild": "*",
    "rollup": "^3 || ^4.0.0",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "chalk": "^5.3.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "rollup": "^4.6.1",
    "tsup": "^8.0.1",
    "vite": "catalog:",
    "webpack": "^5.89.0"
  }
}
