{
  "name": "inclusion-vapor-shared",
  "description": "shared utils for inclusion-vapor",
  "version": "0.0.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/inclusion-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/inclusion-vapor.git",
    "directory": "packages/shared"
  },
  "keywords": [
    "jsx",
    "vapor",
    "compiler"
  ],
  "homepage": "https://github.com/kazupon/inclusion-vapor#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18.18"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "@babel/types": "catalog:",
    "@vue-vapor/compiler-dom": "latest",
    "@vue-vapor/compiler-vapor": "latest",
    "@vue-vapor/shared": "latest"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.7",
    "tsdown": "catalog:"
  }
}
