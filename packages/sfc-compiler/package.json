{
  "name": "svelte-vapor-sfc-compiler",
  "description": "SFC-flavoured vapor svelte component compiler",
  "private": true,
  "version": "0.0.0",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/svelte-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/svelte-vapor.git",
    "directory": "packages/sfc-compiler"
  },
  "keywords": [
    "svelte",
    "vapor",
    "sfc",
    "component",
    "compiler"
  ],
  "homepage": "https://github.com/kazupon/svelte-vapor#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18.18"
