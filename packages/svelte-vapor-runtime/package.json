{
  "name": "svelte-vapor-runtime",
  "description": "svelte runtime for vapor",
  "version": "0.0.4",
  "author": {
    "name": "kazuya kawaguchi",
    "email": "kawakazu80@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/kazupon",
  "bugs": {
    "url": "https://github.com/kazupon/inclusion-vapor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kazupon/inclusion-vapor.git",
    "directory": "packages/svelte-vapor-runtime"
  },
  "keywords": [
    "svelte",
    "runtime",
    "vapor"
  ],
  "homepage": "https://github.com/kazupon/inclusion-vapor#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 18.18"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./action": {
      "types": "./dist/action.d.ts",
      "import": "./dist/action.js",
      "require": "./dist/action.cjs"
    },
    "./animate": {
      "types": "./dist/animate.d.ts",
      "import": "./dist/animate.js",
      "require": "./dist/animate.cjs"
    },
    "./easing": {
      "types": "./dist/easing.d.ts",
      "import": "./dist/easing.js",
      "require": "./dist/easing.cjs"
    },
    "./motion": {
      "types": "./dist/motion.d.ts",
      "import": "./dist/motion.js",
      "require": "./dist/motion.cjs"
    },
    "./store": {
      "types": "./dist/store.d.ts",
      "import": "./dist/store.js",
      "require": "./dist/store.cjs"
    },
    "./transition": {
      "types": "./dist/transition.d.ts",
      "import": "./dist/transition.js",
      "require": "./dist/transition.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "build": "tsdown"
  },
  "dependencies": {
    "@vue-vapor/reactivity": "latest",
    "@vue-vapor/vapor": "latest"
  },
  "devDependencies": {
    "svelte": "catalog:",
    "tsdown": "catalog:"
  }
}
